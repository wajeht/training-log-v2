<section class="px-4 py-5">
  <div class="container">
    <div class="row gy-3 position-relative">
      <!-- Account settings -->
      <div class="col-12 col-md-3 col-lg-3">
        <div class="card shadow-sm">
          <div class="list-group" id="list-tab" role="tablist">
            <div class="card-body">
              <h5 class="card-title mb-3">Dashboard</h5>
            </div>

            <!--  user list -->
            <a
              class="list-group-item list-group-item-action active"
              id="list-user-list"
              data-bs-toggle="list"
              href="#list-users"
              role="tab"
              aria-controls="list-users"
              >Users</a
            >

            <!-- video list -->
            <a
              class="list-group-item list-group-item-action"
              id="list-videos-list"
              data-bs-toggle="list"
              href="#list-videos"
              role="tab"
              aria-controls="list-videos"
              >Videos</a
            >
          </div>
        </div>
      </div>

      <div class="col-12 col-md-9 col-lg-9">
        <div class="tab-content" id="nav-tabContent">
          <!-- dashboard users -->
          <%- include('../../partials/auth/dashboard-users-table.ejs') %>

          <!-- dashboard videos -->
          <%- include('../../partials/auth/dashboard-videos-table.ejs') %>
        </div>
      </div>
    </div>
  </div>
</section>
